<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
           point="org.eclipse.help.contexts">
        <contexts
              file="contexts.xml"
              plugin="com.devepos.adt.searchandanalysistools.doc">
        </contexts>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml">
      </toc>
      <toc
            file="primaryToc.xml"
            primary="true">
      </toc>
      <tocIcon
            closedIcon="icons/toc/toc_closed.gif"
            id="com.devepos.adt.saat.ui.tocIcon"
            openIcon="icons/toc/toc_open.gif">
      </tocIcon>
   </extension>
   <extension
         point="com.devepos.adt.releasenotes.releaseNotes">
      <editorConfig
            editorTitle="ABAP Search and Analysis Tools"
            releaseNotesId="v2.0.0 - 01.10.2023"
            releaseNotesPath="html/changelog/latest-release.html">
      </editorConfig>
   </extension>
</plugin>
