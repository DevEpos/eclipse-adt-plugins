<?xml version="1.0" encoding="UTF-8"?>
<contexts>
   <context id="tag_wizard" title="Tag Objects Wizard">
      <topic href="html/tag_wizard/index.html" label="Tag Objects Wizard"/>
   </context>
   <context id="import_tags_wizard">
      <description>Wizard for importing ABAP Tags and Tagged Objects from another project or a file</description>
      <topic href="html/import_tags_wizard/index.html" label="Import ABAP Tags &amp; Tagged Objects"/>
   </context>
   <context id="import_tags_wiz_source_sel">
      <description>On this page you choose from which to import ABAP Tags / Tagged Objects</description>
      <topic href="html/import_tags_wizard/source_selection.html" label="Select Import Source"/>
   </context>
   <context id="import_tags_wiz_content_sel">
      <description>On this page you select the ABAP Tags and Tagged Objects you want to import</description>
      <topic href="html/import_tags_wizard/content_selection.html" label="Select Content for Import" />
   </context>
   <context id="export_tags_wizard">
      <description>Wizard for exporting  ABAP Tags and Tagged Objects to a file</description>
      <topic href="html/export_tags_wizard/index.html" label="Export ABAP Tags Wizard"/>
   </context>
   <context id="export_tags_wizard_content_sel">
      <description>On this page you select ABAP Tags you want to export</description>
      <topic href="html/export_tags_wizard/tag_selection.html" label="Content Selection for Export"/>
   </context>
   <context id="unassign_tags_wizard_tag_selection">
      <description>On this page you select ABAP Tags to find Objects that have those Tags assigned</description>
      <topic href="html/unassign_tags_wizard/tag_selection.html" label="Select ABAP Tags"/>
   </context>
   <context id="unassign_tags_wizard_object_selection">
      <description>On this page you select the tagged repository objects whose tags shall be removed</description>
      <topic href="html/unassign_tags_wizard/object_selection.html" label="Select Tagged Objects"/>
   </context>
   <context id="unassign_tags_wizard">
      <topic href="html/unassign_tags_wizard/index.html" label="Remove Tags from Objects Wizard"/>
   </context>
   <context id="tag_wizard_object_selection">
      <description>On this page you select the ADT objects to which you want to assign tags</description>
      <topic href="html/tag_wizard/object_selection.html" label="Select Objects"/>
   </context>
   <context id="tag_manager" title="ABAP Tag Manager">
      <topic href="html/tag_manager.html" label="Tag Manager"/>
   </context>
   <context id="object_tags" title="Object Tags View">
      <topic href="html/object_tags.html" label="Object Tags"/>
   </context>
   <context id="tagged_object_search">
      <topic href="html/tagged_object_search.html" label="Tagged Object Search"/>
   </context>
   <context id="tag_wizard_tag_selection">
      <description>On this page you select the tags you want to assign to your repository object. You can also add entirely new tags on this page</description>
      <topic href="html/tag_wizard/tag_selection.html" label="Select ABAP Tags"/>
   </context>
   <context id="tag_wizard_parent_object_selection">
      <description>On this page you select the parent objects for your chosen hierarchical tags.</description>
      <topic href="html/tag_wizard/parent_object_selection.html" label="Select Parent Object"/>
   </context>
